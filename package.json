{
  "name": "openbook-crm",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node --loader ts-node/esm server/server.ts",
    "build": "run-p type-check \"build-only {@}\" build:server --",
    "build:server": "tsc -p tsconfig.node.json",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "start": "node dist/server/server.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@vuepic/vue-datepicker": "^10.0.0",
    "@vueuse/core": "^12.2.0",
    "@vueuse/motion": "^2.2.6",
    "aos": "^2.3.4",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "gsap": "^3.12.5",
    "pinia": "^2.3.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/aos": "^3.0.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.17.12",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/eslint-plugin": "^1.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.0.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vitest": "^1.0.4",
    "vue-tsc": "^2.2.0"
  }
}